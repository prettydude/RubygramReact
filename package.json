    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-cli": "^6.1.3",
    "sass-loader": "^8.0.2",
    "semantic-release": "^17.4.2",
    "serve": "^11.3.2",
    "shx": "^0.3.3",
    "style-loader": "^1.3.0",
    "terser-webpack-plugin": "^4.2.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dashboard": "^3.3.1",
    "webpack-dev-server": "^3.11.2",
    "workbox-webpack-plugin": "^5.1.4"
  }
