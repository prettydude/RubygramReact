{
  "name": "rubygram",
  "version": "1.0.0",
  "description": "Rubygram frontend",
  "main": "''",
  "engines": {
    "node": "14.11.0"
  },
  "scripts": {
    "clean": "shx rm -rf build",
    "webpack": "webpack --config config/webpack.config.production.js",
    "sass": "node-sass src/styles/index.scss build/index.css --output-style compressed",
    "build": "npm run clean && npm run sass && npm run webpack",
    "lint": "eslint src config",
    "dev": "del-cli --force ./build && webpack-dev-server --config config/webpack.config.dev.js",
    "start": "serve -s build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prettydude/nulp-timetable"
  },
  "author": "prettydude",
  "license": "MIT",
  "homepage": "https://prettydude.github.io/nulp-timetable",
  "dependencies": {
    "@keymastervn/redux-token-auth": "^0.19.5",
    "@rails/actioncable": "^6.1.3-1",
    "@reduxjs/toolkit": "^1.5.1",
    "axios": "^0.21.1",
    "history": "^4.10.1",
    "idb": "^5.0.8",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-search-panel": "^1.0.8",
    "react-textarea-autosize": "^8.3.2",
    "redux": "^4.1.0"
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "moduleNameMapper": {
      "\\.(jpe?g|png|gif|eot|otf|webp|svg|ttf|woff2?|mp[34]|webm|wav|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|s[ac]ss|styl)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/runtime": "^7.13.17",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-polyfill": "6.26.0",
    "classnames": "^2.3.1",
    "copy-webpack-plugin": "^5.1.2",
    "css-loader": "^3.6.0",
    "del-cli": "^3.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.5.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-babel-module": "^5.3.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-standard": "^4.1.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
